{
	"name": "stefanjoebstl/joeee_booking",
	"description": "A booking/reservation plugin for hostels, hotels and so on.",
	"type": "project",
	"license": "GPL-3.0-or-later",
	"authors": [
		{
			"name": "Stefan JÃ¶bstl",
			"email": "stefan.joebstl@gmx.at",
			"homepage": "https://joebstl.services",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=7.2.0",
		"tgmpa/tgm-plugin-activation": "~2.6",
		"inpsyde/wonolog": "^1.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^7.3",
		"brain/monkey": "^2.2",
		"mockery/mockery": ">=0.9 <2"
	},
	"minimum-stability": "stable",
	"archive": {
		"exclude": [
			"!vendor/*",
			"node_modules/*",
			"*.zip",
			".gitignore",
			".github",
			".travis.yml",
			"composer.phar",
			"composer.json",
			"composer.lock",
			"phpunit.xml",
			"README.md"
		]
	},
	"autoload": {
		"psr-4": {
			"Joeee_Booking\\": "src/"
		}
	},
	"config": {
		"archive-format": "zip",
		"optimize-autoloader": true
	}
